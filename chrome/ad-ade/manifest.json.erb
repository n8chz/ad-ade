{
    "name": "<%= name %>",
    "version": "<%= version %>",
    "manifest_version": 2,
    "description": "<%= description %>",
    "icons": {
        "16": "common/icon-16.png",
        "48": "common/icon-48.png",
        "128": "common/icon-128.png"
    },
    "homepage_url": "<%= website %>",
    "browser_action": {
        "default_icon": "common/icon-19.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "css": ["common/content.css", "common/fancybox/jquery.fancybox.css"],
            "js": ["common/jquery.min.js", "common/fancybox/jquery.fancybox.pack.js", "common/adreplacer.js", "content.js"]
        }
      ],
    "permissions": [
        "tabs",
        "http://*/*"
    ],
    "web_accessible_resources": [
    ],
    "incognito": "spanning"
}